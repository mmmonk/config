# Defaults
host *
#  AddressFamily inet
  CheckHostIP yes
  Cipher blowfish
  Ciphers aes256-ctr,3des-cbc,blowfish-cbc
  Compression yes
  ConnectTimeout 30
  ConnectionAttempts 2
  ControlPersist 30m
  ControlMaster auto
  ControlPath ~/.ssh/sockets/%r@%h:%p
  ForwardAgent no
  ForwardX11 no
  ForwardX11Trusted no
  GSSAPIAuthentication no
  HashKnownHosts yes
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_ecdsa
  IdentityFile ~/.ssh/id_rsa_2010
  IdentityFile ~/.ssh/id_rsa_%h
  KeepAlive yes
  LogLevel VERBOSE
  MACs hmac-ripemd160,hmac-sha1,hmac-md5
  NoHostAuthenticationForLocalhost yes
  PermitLocalCommand yes
  Port 22
  PreferredAuthentications publickey,password,keyboard-interactive
  Protocol 2
  ProxyCommand none
  #ProxyCommand /home/case/bin/nagle %h %p
  RekeyLimit 512M
  SendEnv ""
  ServerAliveCountMax 3
  ServerAliveInterval 10
  StrictHostKeyChecking ask
  TCPKeepAlive yes
  TunnelDevice any:any
  User case
  VerifyHostKeyDNS ask
  VisualHostKey no
